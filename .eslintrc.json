{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "class-methods-use-this": "off",
    "no-unused-vars": ["error", { "args": "none" }],
    "no-undef": "off",
    "no-underscore-dangle": "off",
    "no-console": "off",
    "max-len": "off"
  }
}
